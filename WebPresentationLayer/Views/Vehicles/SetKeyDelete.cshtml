
@{
    ViewData["Title"] = "Key";
}

<h1>Enter key to confirm deleting</h1>


<form asp-action="SetKeyDelete">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="form-group">
        <label class="control-label">Vehicle Key</label>
        <input class="form-control" name="vehicleKey" id="vehicleKey"/>
        <span class="text-danger"></span>
    </div>
    <div class="mt-3 col-md-12">
    </div>
    <div class="form-group">
        <input type="submit" value="Enter" class="btn btn-primary shadow-none" />
    </div>
</form>